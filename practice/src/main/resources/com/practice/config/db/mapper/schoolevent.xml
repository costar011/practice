<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="event">

	<select id="schoolevent" resultType="schooleventVO">
		SELECT UID
		,TITLE
		,WRITTIN
		FROM SCHOOLEVENT
	</select>


	<insert id="schoolInsert" parameterType="schooleventVO">
		INSERT INTO SCHOOLEVENT
		(
		TITLE,
		WRITTIN
		)
		VALUES
		(
		#{title},
		#{writtin}
		)

	</insert>

	<delete id="schooldelet" parameterType="String">

		DELET FROM SCHOOLEVENT
		WHERE UID = #{uid}
	</delete>


</mapper>

