<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
   <mapper namespace="communtiyplace">
   
	<select id="selectComm" resultType="commVO">
		
		SELECT	A.UID,
				A.TITLE,
       		    A.WRITTEN,
        	 	B.`NAME`
    	  FROM	TB_BOARD	A
 		 INNER
  		  JOIN	`MEMBER`	B
    		ON	A.MEMBER_UID = B.MEMBER_UID;
	
	</select>
	</mapper>