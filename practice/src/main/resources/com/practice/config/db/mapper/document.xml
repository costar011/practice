<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
   <mapper namespace="document">
   
   <select id="documentAllList" resultType="DocumentVO">
  	
  		SELECT UID,
  			   TITLE,
  			   WRITTIN
  		  FROM SCHOOLDOCUMENT
  		
  	</select>
  	
  	<insert id="insertdocument" parameterType="DocumentVO">
  		INSERT INTO SCHOOLDOCUMENT
  		(
  			TITLE
  			,WRITTIN
  		)
  		VALUES
  		(
  			'${title }'
  			,'${writtin }'
  		)
  	</insert>  	
  	
  	<delete id="deletedocument" parameterType="string">
  	
  	DELECT FROM SCHOOLDOCUMENT
  	 WHERE UID = ${uid}
  	 
  	</delete>
  	
   </mapper>
  