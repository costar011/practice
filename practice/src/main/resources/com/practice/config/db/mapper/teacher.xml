<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="teacher">

	<select id="teacherAllList" resultType="teacherVO">
		SELECT	UID
				, NAME
				, MOBILE
				, SEX
				, JOB
				, MGR
				, ADDRESS
				, EMAIL
				, SAL
		  FROM	TEACHER
	</select>
	

</mapper>

